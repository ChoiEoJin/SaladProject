<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

<typeAliases>
     <typeAlias alias="Talk" type="talk.model.TalkBean"/>
     <typeAlias alias="Member" type="member.model.MemberBean"/>
     <typeAlias alias="MyProduct" type="product.model.ProductBean"/>
     <typeAlias alias="Cart" type="cart.model.cartBean"/>
     <typeAlias alias="Pay" type="pay.model.PayBean"/>
     <typeAlias alias="MyBoard" type="board.model.BoardBean"/>
     <typeAlias alias="MyReview" type="board.model.ReviewBean"/>
     <typeAlias alias="MyReviewReply" type="board.model.ReviewReplyBean"/>
     <typeAlias alias="MyReviewLike" type="board.model.ReviewLikeBean"/>
     <typeAlias alias="addressBean" type="pay.model.addressBean"/>
     
</typeAliases>

<mappers>
     <mapper resource="alias/mybatis/talkmapper.xml" />
     <mapper resource="alias/mybatis/membermapper.xml"/>
     <mapper resource="alias/mybatis/productmapper.xml"/>
     <mapper resource="alias/mybatis/cartmapper.xml"/>
     <mapper resource="alias/mybatis/saladmapper.xml"/>
     <mapper resource="alias/mybatis/paymapper.xml"/>
     <mapper resource="alias/mybatis/BoardMapper.xml"/>
     <mapper resource="alias/mybatis/BoardReviewMapper.xml"/>
     <mapper resource="alias/mybatis/BoardReviewReplyMapper.xml"/>
     <mapper resource="alias/mybatis/BoardReviewLikeMapper.xml"/>
     
</mappers>

</configuration>