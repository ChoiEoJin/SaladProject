<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="alias.mybatis.saladmapper">
	

	<select id="getVegetable" resultType="MyProduct" parameterType="String">
	select * from product where category='vegetable'
	</select>
	
	<select id="getFruitList" resultType="MyProduct" parameterType="String">
	select * from product where category='fruit'
	</select>

	<select id="getSideMenu" resultType="MyProduct" parameterType="String">
	select * from product where category='sidemenu'
	</select>
	
	<select id="getTopping" resultType="MyProduct" parameterType="String">
	select * from product where category='topping'
	</select>	
	
	<select id="getDressing" resultType="myProduct" parameterType="String">
	select * from product where category='dressing'
	</select>
</mapper>